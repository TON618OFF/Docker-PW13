# Переменные окружения для ImperialTunes
# Скопируйте этот файл в .env.local и заполните значения

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Database Configuration (для локальной разработки)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=imperial_tunes
DB_USER=postgres
DB_PASSWORD=postgres

# Application Configuration
VITE_APP_NAME=ImperialTunes
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Музыкальный плеер с аналитикой

# Storage Configuration
VITE_MAX_FILE_SIZE=52428800
VITE_ALLOWED_AUDIO_FORMATS=mp3,wav,flac,ogg,m4a
VITE_ALLOWED_IMAGE_FORMATS=jpeg,png,webp

# Analytics Configuration
VITE_ANALYTICS_ENABLED=true
VITE_ANALYTICS_RETENTION_DAYS=365

# Security Configuration
VITE_ENABLE_AUDIT_LOG=true
VITE_SESSION_TIMEOUT=3600

# Development Configuration
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info

